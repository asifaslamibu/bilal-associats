    <script>
        function changeFunc1() {
            var selectBox = document.getElementById("t_brand");
            var selectedValue = selectBox.options[selectBox.selectedIndex].value;
            console.log("han " + selectedValue);
            //    $.get('/area?id=' + selectedValue );
            $.ajax({
                type: 'GET',
                url: '/t_num?id=' + selectedValue,
                dataType: 'json',
                success: function (response) {

                    var len = response.length;

                    $("#t_num").empty();
                    for (var i = 0; i < len; i++) {

                        var id = response[i]['id'];
                        var no = response[i]['product_no'];
                        console.log(id + "  " + no)
                        //$("#area2").append("<option value='"+id+"'>"+name+"</option>");
                        $('#t_num').append(`<option value="${id}">${no}</option>`);

                    }
                },
                error: function (data) {
                    console.log("Error.", data);
                }
            });

            function getData() {
                return $.ajax({
                    url: '/check_tyre_quan?id=' + selectedValue,
                    type: 'GET'
                });
            }

            function handleData(data /* , textStatus, jqXHR */) {
                // alert(data[0]['oil_name']);
                var id = data['id'];
                var name = data['product_name'];
                var quantity = data['quantity'];
                check_Q = quantity;

                console.log("Tyre quantity " + check_Q);

                var btn = document.getElementById("btn");
                btn.addEventListener("click", function () {
                    var i_date = document.getElementById("basicFlatpickr").value;
                    var v_no = document.getElementById("v_no").value;
                    var v_type = document.getElementById("v_type").value;
                    var Quatities = document.getElementById("qty").value;
                    var remark = document.getElementById("remark").value;
                    var t_num = document.getElementById("t_num").value;
                    var t_brand = document.getElementById("t_brand").value;
                    var WareHouse = document.getElementById("WareHouse").value;
                    var c_odo = document.getElementById("c_odo").value;
                    var n_odo = document.getElementById("n_odo").value;
                    var expect_date = document.getElementById("basicFlatpickr1").value;

                    console.log("Andar " + Quatities);
                    if (check_Q>=Quatities) {
                        return $.ajax({
                            url: '/create_tyre_issue?i_date=' + i_date + '&v_no=' + v_no + '&v_type=' + v_type + '&Quatities=' + Quatities + '&remark=' + remark + '&t_num=' + t_num + '&t_brand=' + t_brand + '&WareHouse=' + WareHouse + '&main_id=' + id + '&total=' + check_Q+ '&c_odo=' + c_odo+ '&n_odo=' + n_odo+ '&expect_date=' + expect_date,
                            type: 'GET'
                        });
                        alert("Submmited ...")
                    }
                    else {
                        alert("Quantity not Available..... Only " + check_Q + " Tyre Available.");
                    }

                })

                //do some stuff
            }

            getData().done(handleData);
        }
    </script>

