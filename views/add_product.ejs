            padding: .75rem 1.25rem;
            border-radius: 6px;
            box-shadow: none;


        }

        .select3::after {
            content: '\25BC';
            position: absolute;
            top: 0;
            right: 0;
            padding: 0 1em;
            background: #f6f7f8;
            cursor: pointer;
            pointer-events: none;
            -webkit-transition: .25s all ease;
            -o-transition: .25s all ease;
            transition: .25s all ease;
        }

        /* Transition */
        .select3:hover::after {
            color: #f6f7f8;
        }
