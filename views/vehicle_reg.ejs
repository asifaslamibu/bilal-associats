            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Vehicle Registration Form</h5>
                 
                </div>
                <form id="_addCustomerReg" novalidate="novalidate" action="/create_vehicle_reg" method="POST">
                    <div class="card-body">
                        <div class="row mb-4">
                            <div class="col">
                                <label for="txtVehicleName">Vechicle No</label>

                                <input type="text" class="form-control" id="v_no" name="v_no">
                            </div>
                            <div class="col">
                                <label for="txtVehicleName">Vehicle Type</label>
<br>
                                <select class="selectpicker" data-live-search="true" data-width="100%"id="v_type" name="v_type">
                                    <option value="0" >Please Select a VehicleType</option>
                                    <option value="Car" >Car</option>
                                    <option value="Truck,Trailor" >Truck,Trailor</option>
                                    <option value="Oil Tanker" >Oil Tanker</option>
                                    <option value="Lifter RS" >Lifter RS</option>
                                    <option value="Fork Lifter" >Fork Lifter</option>
                                    <option value="Crane" >Crane</option>
                                    <option value="LTV" >LTV</option>
                                    <option value="Motorcycle" >Motorcycle</option>
                                    <option value="OP">OP</option>
                                </select>


                            </div>
                            
                        </div>
                       
                            <div class="row mb-4">
                                <div class="col">
                                    <label for="txtVehicleName">Engine No</label>
    
                                    <input type="text" class="form-control" id="e_no" name="e_no">
                                </div>
                                <div class="col">
                                    <label for="txtVehicleName">Chassis No</label>
    
                                    <input type="text" class="form-control"id="c_no" name="c_no">
                                </div>
                                <div class="col">
                                    <label for="txtVehicleName">Current Odometer</label>
    
                                    <input type="text" class="form-control"id="odo_c" name="odo_c">
                                </div>
                            </div>
                            <div class="row mb-4">
                                <div class="col">
                                    <label for="txtVehicleName">Vehicle Name</label>
    
                                    <input type="text" class="form-control"id="v_name" name="v_name">
                                </div>
                                <div class="col-lg-6">
                                    <div class="n-chk">
                                        <label class="new-control new-checkbox checkbox-outline-success">
                                            <input type="checkbox" class="new-control-input" id="status" name="status">
                                            <span class="new-control-indicator"></span>Status
                                        </label>
                                    </div>
                                </div>
                                
                            </div>
    
                          n>
